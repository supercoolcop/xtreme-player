{
  "expo": {
    "name": "Xtream IPTV Player",
    "slug": "xtream-player",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2196F3"
    },
    "platforms": [
      "ios",
      "android"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.blcharity.xtreamiptvplayer",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": [],
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true,
          "NSAllowsArbitraryLoadsInMedia": true
        }
      },
      "entitlements": {
        "aps-environment": "development"
      },
      "buildNumber": "1.5"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#2196F3"
      },
      "package": "org.blcharity.xtreamiptvplayer",
      "versionCode": 1
    },
    "extra": {
      "eas": {
        "projectId": "4448c79b-7f57-4024-80aa-19508600090d"
      }
    },
    "owner": "supercoolcop"
  }
}
